<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">

    <f:entry title="${%label}" field="label">
        <f:textbox/>
    </f:entry>
    <f:entry title="${%hostname}" field="hostname">
        <f:textbox/>
    </f:entry>
    <f:entry title="${%enabled}" field="enabled">
        <f:checkbox default="${descriptor.getDefaultEnabled}" />
    </f:entry>

</j:jelly>